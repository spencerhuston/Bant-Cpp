func double[T](l: List[T], size: int, curr: int) -> List[T] = {
	if (size == curr)
		l
	else
		double(pushBack(l, l[curr]), size, curr + 1)	
};

val arr : List[int] = new List { 1, 2, 3 };
val arr2 : List[int] = double(arr, size(arr), 0);

val arr3 : List[bool] = new List { true, false };
val arr4 : List[bool] = double(arr3, size(arr3), 0);

if ((arr2[5] == 3) && (arr4[3] == false))
	2
else
	1

