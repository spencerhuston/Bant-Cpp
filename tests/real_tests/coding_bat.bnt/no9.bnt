func no9(size: int, curr: int, arr: List[int], newlist: List[int]) -> List[int] = {
	func getLast(num: int) -> int = {
		if (num < 10)
			num
		else
			getLast(num % 10)
	};

	if (curr >= size)
		newlist
	else if (getLast(arr[curr]) != 9)
		no9(size, curr + 1, arr, pushBack(newlist, arr[curr]))
	else
		no9(size, curr + 1, arr, newlist)
};

val list : List[int] = no9(5, 0, new List {9,13,29,3,19}, new List { });

if ((list[0] == 13) && (list[1] == 3))
	3
else
	9
